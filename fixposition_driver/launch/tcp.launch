<launch>
    <!--Change input_type and input_port according to the type of input source:
               tcp - data over TCP
               serial - data over a serial port 
        And according to the port used for reading, i.e.
               21000 - TCP port
               /dev/ttyUSB0 - serial port -->

    <node name="fixposition_driver" pkg="fixposition_driver" exec="fixposition_driver_exec" output="screen" respawn="true" respawn_delay="5">
    <param from="$(find-pkg-share fixposition_driver)/launch/tcp.yaml" />
    </node>
</launch>
